/* ── Global ───────────────────────────────── */
* { font-family: 'Segoe UI', Arial, sans-serif; }
QMainWindow, QWidget#centralwidget, QWidget#bodyWidget {
    background: #f1f5f9;
    color: #1e293b;
	
	background-color: rgb(0, 0, 127);
}

/* ── Header ───────────────────────────────── */
QWidget#headerWidget {
    background: white;
    border-bottom: 1px solid #e2e8f0;
}
QLabel#appTitle {
    font-size: 17px;
    font-weight: 700;
    color: #0f172a;
}
QLabel#hlbl, QLabel#connText {
    font-size: 11px;
    color: #64748b;
}

/* ── Botón Conectar ───────────────────────── */
QPushButton#btnConnect {
    background: #2563eb;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 8px 0px;
    font-weight: 600;
    font-size: 13px;
    min-width: 115px;
}
QPushButton#btnConnect:hover   { background: #1d4ed8; }
QPushButton#btnConnect:checked { background: #dc2626; }
QPushButton#btnConnect:checked:hover { background: #b91c1c; }

/* ── Botón ícono refresh ──────────────────── */
QPushButton#btnRefresh {
    background: #f8fafc;
    color: #475569;
    border: 1px solid #e2e8f0;
    border-radius: 8px;
    font-size: 16px;
    font-weight: bold;
    min-width: 34px;
    min-height: 34px;
    max-width: 34px;
    max-height: 34px;
}
QPushButton#btnRefresh:hover { background: #f1f5f9; }

/* ── Cards (GroupBox) ─────────────────────── */
QGroupBox {
    background: white;
    border: 1px solid #e2e8f0;
    border-radius: 12px;
    margin-top: 16px;
    font-size: 11px;
    font-weight: 600;
    color: #64748b;
    padding: 4px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 14px;
    padding: 0 6px;
    background: white;
}

/* ── Botones generales ────────────────────── */
QPushButton {
    background: #f8fafc;
    color: #334155;
    border: 1px solid #e2e8f0;
    border-radius: 7px;
    padding: 6px 14px;
    font-size: 12px;
}
QPushButton:hover   { background: #f1f5f9; border-color: #cbd5e1; }
QPushButton:pressed { background: #e2e8f0; }

QPushButton#btnPing {
    background: #2563eb; color: white; border: none;
    border-radius: 7px; padding: 7px 18px; font-weight: 600;
}
QPushButton#btnPing:hover { background: #1d4ed8; }

QPushButton#btnReset {
    background: #dc2626; color: white; border: none;
    border-radius: 7px; padding: 7px 18px; font-weight: 600;
}
QPushButton#btnReset:hover { background: #b91c1c; }

QPushButton#btnSendServo, QPushButton#btnSendRaw {
    background: #2563eb; color: white; border: none;
    border-radius: 7px; font-weight: 700;
}
QPushButton#btnSendServo:hover, QPushButton#btnSendRaw:hover { background: #1d4ed8; }

QPushButton#btnNeoOff    { background:#9ca3af; color:white;   border:none; border-radius:7px; font-weight:600; font-size:11px; padding:7px 4px; }
QPushButton#btnNeoRojo   { background:#ef4444; color:white;   border:none; border-radius:7px; font-weight:600; font-size:11px; padding:7px 4px; }
QPushButton#btnNeoVerde  { background:#22c55e; color:white;   border:none; border-radius:7px; font-weight:600; font-size:11px; padding:7px 4px; }
QPushButton#btnNeoAzul   { background:#3b82f6; color:white;   border:none; border-radius:7px; font-weight:600; font-size:11px; padding:7px 4px; }
QPushButton#btnNeoBlanco { background:#f1f5f9; color:#1e293b; border:none; border-radius:7px; font-weight:600; font-size:11px; padding:7px 4px; }

QPushButton#btnClearLog {
    background: #f1f5f9; color: #475569;
    border: 1px solid #e2e8f0; border-radius: 6px;
    padding: 4px 10px; font-size: 11px;
}

/* ── Botones send por motor ───────────────── */
QPushButton#btnSendM1, QPushButton#btnSendM2,
QPushButton#btnSendM3, QPushButton#btnSendM4 {
    background: #2563eb; color: white; border: none;
    border-radius: 6px; font-weight: 700; font-size: 13px;
    min-width: 28px; max-width: 28px;
    min-height: 28px; max-height: 28px;
}
QPushButton#btnSendM1:hover, QPushButton#btnSendM2:hover,
QPushButton#btnSendM3:hover, QPushButton#btnSendM4:hover { background: #1d4ed8; }

/* ── Botones Digital tiny ─────────────────── */
QPushButton#btnOn11,  QPushButton#btnOn12,  QPushButton#btnOn21,
QPushButton#btnOn22,  QPushButton#btnOn31,  QPushButton#btnOn32,
QPushButton#btnOn41,  QPushButton#btnOn42 {
    background: #22c55e; color: white; border: none;
    border-radius: 5px; font-size: 10px; font-weight: 600;
    min-width: 26px; max-width: 26px; min-height: 22px; max-height: 22px;
    padding: 0;
}
QPushButton#btnOff11, QPushButton#btnOff12, QPushButton#btnOff21,
QPushButton#btnOff22, QPushButton#btnOff31, QPushButton#btnOff32,
QPushButton#btnOff41, QPushButton#btnOff42 {
    background: #f1f5f9; color: #475569; border: 1px solid #e2e8f0;
    border-radius: 5px; font-size: 10px;
    min-width: 26px; max-width: 26px; min-height: 22px; max-height: 22px;
    padding: 0;
}

/* ── Labels ───────────────────────────────── */
QLabel#motorLbl { font-size: 11px; font-weight: 700; color: #475569; }
QLabel#bigVal   { font-size: 14px; font-weight: 700; color: #0f172a; }
QLabel#sectionLbl {
    font-size: 10px; font-weight: 600;
    color: #94a3b8; text-transform: uppercase;
}
QLabel#fbTitle  { font-size: 11px; font-weight: 600; color: #64748b; }
QLabel#neoLabel { font-size: 12px; font-weight: 600; color: #1e293b; }

/* ── ComboBox ─────────────────────────────── */
QComboBox {
    background: #f8fafc; color: #1e293b;
    border: 1px solid #e2e8f0; border-radius: 7px;
    padding: 5px 10px; font-size: 12px; min-width: 110px;
}
QComboBox:hover { border-color: #2563eb; }
QComboBox::drop-down { border: none; width: 18px; }
QComboBox QAbstractItemView { background: white; border: 1px solid #e2e8f0; }

/* ── LineEdit ─────────────────────────────── */
QLineEdit {
    background: #f8fafc; color: #1e293b;
    border: 1px solid #e2e8f0; border-radius: 7px;
    padding: 6px 10px; font-size: 12px;
}
QLineEdit:focus { border-color: #2563eb; background: white; }

/* ── Sliders ──────────────────────────────── */
QSlider::groove:horizontal {
    height: 5px; background: #e2e8f0; border-radius: 3px;
}
QSlider::handle:horizontal {
    background: #2563eb;
    width: 15px; height: 15px;
    margin: -5px 0; border-radius: 8px;
    border: 2px solid white;
}
QSlider::sub-page:horizontal { background: #93c5fd; border-radius: 3px; }

/* ── Terminal ─────────────────────────────── */
QTextEdit#terminal {
    background: #0f172a; color: #94a3b8;
    border: 1px solid #1e293b; border-radius: 8px;
    font-family: 'Courier New', monospace;
    font-size: 10px;
    padding: 4px;
}

/* ── Status bar ───────────────────────────── */
QStatusBar {
    background: white;
    color: #94a3b8;
    border-top: 1px solid #e2e8f0;
    font-size: 11px;
}
